<?xml version="1.0" encoding="UTF-8"?>

<launch>
     <!-- Robot state publisher -->
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
      <param name="publish_frequency" type="double" value="50.0"/>
    </node>

    <arg name="arm"  default="True"/>
    <arg name="ft_sensor"  default="schunk-ft"/>
    <arg name="end_effector"  default="pal-gripper"/>
    <arg name="camera_model"  default="xtion"/>
    <arg name="wrist_model"  default="wrist-2010"/>
    <arg name="laser_model"  default="sick-571"/>

    <!-- Joint trajectory and diff drive controllers -->
    <rosparam command="load" file="$(find tiago_controller_configuration_gazebo)/config/mobile_base_controller.yaml"/>
    <include file="$(find tobi_sim)/launch/tiago/tiago_controllers.launch">
        <arg name="arm" value="$(arg arm)"/>
        <arg name="end_effector" value="$(arg end_effector)"/>
        <arg name="ft_sensor" value="$(arg ft_sensor)"/>
        <arg name="laser_model" value="$(arg laser_model)"/>
        <arg name="camera_model" value="$(arg camera_model)"/>
        <arg name="use_moveit_camera" value="False"/>
    </include>

    <include file="$(find tiago_clf_nav)/launch/filters.launch">
          <arg name="sim" value="True"/>
    </include>

    <node name="rviz" args="-d $(find test)/config/protoconfig.rviz" pkg="rviz" type="rviz" respawn="false" output="screen">
    </node>

</launch>
